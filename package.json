        "concurrently": "^5.3.0"
    },
    "dependencies": {
        "@apollo/client": "^3.7.14",
        "apollo-server-express": "^3.12.0",
        "bcrypt": "^5.1.0",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "jsonwebtoken": "^9.0.0",
        "mongoose": "^7.2.0"
    }
}
